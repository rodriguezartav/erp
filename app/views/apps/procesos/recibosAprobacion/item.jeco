<% Cliente = require("models/cliente")  %>
<% cliente = Cliente.find(@Cliente)  %>

<tr>
<td><p><%= @Referencia %><p></td>
<td><p><%= cliente.Name %><p></td>
<td><p><%= @Total.toMoney() %><p></td>
<td><a data-referencia="<%= @Referencia %>" data-aprobar="1"  class="btn btn-primary aprobar">Aprobar</a></td>
<td><a data-referencia="<%= @Referencia %>" data-aprobar="0"  class="btn btn-danger archivar">Archivar</a></td>
</tr>