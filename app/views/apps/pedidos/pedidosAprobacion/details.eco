<% Producto = require("models/producto")  %>

<table class="table table-striped table-bordered table-condensed">
  <tbody>
   
<% for pedido in @: %>

  <% producto = Producto.exists(pedido.Producto)  %>

  <tr>
    <td class="center"><strong><%= pedido.Cantidad %></strong></td>
    <td><%= producto.Name %></td>
    <td class="center"><%= pedido.Precio.toMoney() %></td>
    <td class="center"><%= pedido.Descuento %>%</td>
   </tr>

<% end %>

</tbody>
</table>
