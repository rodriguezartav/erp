
<table class="table table-striped table-bordered table-condensed">
  <tbody>

    <% for saldo in @: %>
      <% if saldo.Saldo != 0 : %>
        <tr>
          <td class="center"><strong><%= saldo.Consecutivo %></strong></td>
          <td class="center"><%= saldo.Tipo_de_Documento %></td>
          <td class="center"><%= saldo.Saldo.toMoney() %></td>
          <td class="center"><%= saldo.PlazoReal() %></td>
         </tr>
       <% end %>
    <% end %>

  </tbody>
</table>
