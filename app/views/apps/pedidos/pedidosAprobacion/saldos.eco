
<table class="table table-striped table-bordered table-condensed">
  <tbody>

    <% for saldo in @: %>
      <tr>
        <td class="center"><strong><%= saldo.CodigoExterno %></strong></td>
        <td class="center"><%= saldo.Saldo.toMoney() %></td>
        <td class="center"><%= saldo.PlazoActual %></td>
       </tr>
    <% end %>

  </tbody>
</table>
