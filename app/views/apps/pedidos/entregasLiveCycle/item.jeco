  <i class="icon-truck btn_entregado"  data-id="<%= @id %>" ></i>
  
  N&#176;:<%= @Consecutivo %> <%= cliente.Name.capitalize() %> <span class="badge badge-warning"><%= @FechaEntregaPropuesta.toMMMDate() %></span>


