<% Spine   = require('spine') %>

<tr>
<td class="center info_popover" rel="popover" data-content="<%= @Observacion %>" data-original-title="Observacion de la Factura">
  <a target="_blank" class="label label-info" href="<%= Spine.session.instance_url + "/" + @id %>">
  <i class="icon-tag"></i>
 <%= @Referencia  %> </a>
</td>
  <td><p><%= @NombreProveedor %><p></td>

  <td><p><%= @FechaVencimiento%><p></td>
  <td>
    <div class="input-append date txtFecha" data-id="<%= @id  %>" data-date="<%= @FechaPagoProgramado%>" data-date-format="yyyy-mm-dd">
        <input data-id="<%= @id  %>" placeholder="Escoja una Fecha" class="span5" size="16" type="text" value="<%= @FechaPagoProgramado%>">
        <span class="add-on"><i class="icon-th"></i></span>
    </div>
  </td>
  
  <td style="text-align: center;">
    <button  data-id="<%= @id  %>" data-saldo="<%= @Saldo %>" style="min-width: 80%" class="btn saldo" data-toggle="button">
      <%= @Saldo.toMoney() %>
    </button>
  </td>

  
</tr>