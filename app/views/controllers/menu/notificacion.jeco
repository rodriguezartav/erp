<li data-userid="<%= @id  %>" data-id="<%= @id  %>" class="notificacion_item <%= @status %>">
  <div class="row-fluid">
    <img src="<%= @image %>"/>
    
    <h6><%= @title %></h6>
    
    <p class="text"><%= @text %></p>
    
    <span class="date"><%= @date.to_pretty() %></span>

  </div>

</li>